
<h1>Here are all the bookings: </h1>
<%= @bookings.each do |booking| %>
<div class="container container-custom">
  <div class="row row-custom">
    <div class="col-6 centering form rounded-input mt-3 p-4 shadow mb-3">
     <p>Name:<%= @booking.name%></p>
     <p>E-mail:<%= @booking.email%></p>
     <p>Start date: <%= @booking.beginning %></p>
     <p>End date: <%= @booking.end %></p>
     <p>Number of seats: <%= @booking.seats %>.</p>
    </div>
  </div>
</div>
<%=  %>
<% end %>
